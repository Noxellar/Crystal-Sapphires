			],
			"settings": {
				"foreground": "#C3E88D"
			}
		},
		{
			// Aquamarine
			"name": "Markup - Italic",
			"scope": [
				"markup.italic"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#7FFFD4"
			}
		},
		{
			// Light Coral
			"name": "Markup - Bold",
			"scope": [

{
	"name": "Neon Midnight",
	"type": "dark",
	"colors": {
		// EDITOR SETTINGS
		"editor.background": "#000021",
		"editor.foreground": "#EEFFFF",
		"editor.selectionHighlightBackground": "#00CED144",
		"editor.findMatchHighlightBackground": "#00CED144",
		"editor.hoverHighlightBackground": "#00CED144",
		"editorGroupHeader.tabsBackground": "#31228841",
		"editorWidget.background": "#00002E",
		"editorSuggestWidget.selectedBackground": "#1A1A48",
		"editorMarkerNavigation.background": "#00002E",
		// EDITOR TAB SETTINGS
		"tab.activeBackground": "#3122884d",
		"tab.inactiveBackground": "#31228811",
		// SIDEBAR SETTINGS
		"sideBar.foreground": "#318CE7",
		"sideBar.background": "#00002E",
		"sideBarSectionHeader.foreground": "#FFFFFF",
		"sideBarTitle.foreground": "#FFFFFF",
		// SOMETHING
		"list.activeSelectionForeground": "#FFFFFF",
		"list.activeSelectionBackground": "#1A1A48",
		"list.inactiveSelectionBackground": "#1A1A48",
		"list.hoverForeground": "#FFFFFF",
		"list.hoverBackground": "#1A1A48",
		// ACTIVITY BAR SETTINGS
		"activityBar.background": "#00004A",
		"activityBar.activeBorder": "#318CE7",
		"activityBar.activeBackground": "#00CED144",
		"activityBarBadge.background": "#007ACC",
		// JSLK
		"input.background": "#00004A",
		// JSDL
		"notifications.background": "#00002E"
	},
	"tokenColors": [
		{
			// Dim Gray
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#696969"
			}
		},
		{
			// Cornflower Blue
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder"
			],
			"settings": {
				"foreground": "#6495ED"
			}
		},
		{
			// White
			"name": "Colors",
			"scope": [
				"constant.other.color"
			],
			"settings": {
				"foreground": "#FFFFFF"
			}
		},
		{
			// YELLOWYELLOWYELLOWYELLOWYELLOWYELLOWYELLOWYELLOWYELLOWYELLOW
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#FFFF00"
			}
		},
		{
			// Lemon Chiffon
			"name": "Keyword, Storage",
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#FFFACD"
			}
		},
		{
			// White
			"name": "Brackets",
			"scope": [
				"punctuation.definition.arguments",
				"punctuation.definition.parameters.begin",
				"punctuation.parenthesis"
			],
			"settings": {
				"foreground": "#FFFFFF"
			}
		},
		{
			// Dark Turquoise
			"name": "HTML Brackets",
			"scope": [
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html"
			],
			"settings": {
				"foreground": "#00CED1"
			}
		},
		{ // Tomato
			"name": "Operator, Misc",
			"scope": [
				"keyword.control",
				"meta.tag",
				"punctuation.definition.tag",
				"punctuation.separator.inheritance.php",
				"punctuation.section.embedded",
				"keyword.other.template",
				"keyword.other.substitution"
			],
			"settings": {
				"foreground": "#FF6347"
			}
		},
		{
			// Cornflower Blue
			"name": "Tag",
			"scope": [
				"entity.name.tag",
				"meta.tag.sgml",
				"markup.deleted.git_gutter"
			],
			"settings": {
				"foreground": "#6495ED"
			}
		},
		{
			// Aqua
			"name": "Function, Special Method",
			"scope": [
				"entity.name.function",
				"meta.function-call",
				"variable.function",
				"support.function",
				"keyword.other.special-method"
			],
			"settings": {
				"foreground": "#00FFFF"
			}
		},
		{
			// Aqua
			"name": "Block Level Variables",
			"scope": [
				"meta.block variable.other"
			],
			"settings": {
				"foreground": "#00FFFF"
			}
		},
		{
			// Tomato
			"name": "Other Variable, String Link",
			"scope": [
				"support.other.variable",
				"string.other.link"
			],
			"settings": {
				"foreground": "#FF6347"
			}
		},
		{
			// Dodger Blue
			"name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
			"scope": [
				"constant.numeric",
				"constant.language",
				"support.constant",
				"constant.character",
				"constant.escape",
				"variable.parameter",
				"keyword.other.unit",
				"keyword.other"
			],
			"settings": {
				"foreground": "#1E90FF"
			}
		},
		{
			// Lime Green
			"name": "String, Symbols, Inherited Class, Markup Heading",
			"scope": [
				"string",
				"constant.other.symbol",
				"constant.other.key",
				"entity.other.inherited-class",
				"markup.heading",
				"markup.inserted.git_gutter",
				"meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
			],
			"settings": {
				"foreground": "#32CD32"
			}
		},
		{
			// Medium Sea Green
			"name": "Class, Support",
			"scope": [
				"entity.name",
				"support.type",
				"support.class",
				"support.orther.namespace.use.php",
				"meta.use.php",
				"support.other.namespace.php",
				"markup.changed.git_gutter",
				"support.type.sys-types"
			],
			"settings": {
				"foreground": "#3CB371"
			}
		},
		{
			// Teal
			"name": "Entity Types",
			"scope": [
				"support.type"
			],
			"settings": {
				"foreground": "#008080"
			}
		},
		{
			// Dark Turquoise
			"name": "CSS Class and Support",
			"scope": [
				"source.css support.type.property-name",
				"source.sass support.type.property-name",
				"source.scss support.type.property-name",
				"source.less support.type.property-name",
				"source.stylus support.type.property-name",
				"source.postcss support.type.property-name"
			],
			"settings": {
				"foreground": "#00CED1"
			}
		},
		{
			// YELLOWYELLOWYELLOWYELLOWYELLOWYELLOWYELLOWYELLOWYELLOWYELLOW
			"name": "Sub-methods",
			"scope": [
				"entity.name.module.js",
				"variable.import.parameter.js",
				"variable.other.class.js"
			],
			"settings": {
				"foreground": "#FFFF00"
			}
		},
		{
			// Spring Green
			"name": "Language methods",
			"scope": [
				"variable.language"
			],
			"settings": {
				"foreground": "#00FF7F"
			}
		},
		{
			// Aqua
			"name": "entity.name.method.js",
			"scope": [
				"entity.name.method.js"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#00FFFF"
			}
		},
		{
			// Aqua
			"name": "meta.method.js",
			"scope": [
				"meta.class-method.js entity.name.function.js",
